<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sizebook"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="25" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="pweckend-SizeBook"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.sizebook.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sizebook.AddNew" />
        <activity android:name="com.example.sizebook.ViewDetails" />
        <activity android:name="com.example.sizebook.UpdateRecord" />
    </application>

</manifest>